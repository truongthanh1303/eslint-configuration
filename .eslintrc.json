{
    "extends": [
      "standard",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "standard",
      "promise",
      "react",
      "react-native"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      }
    }
}
